.parkPay{
}
.search{
  display: flex;
  justify-content: center;
  width: 100%;
  position: fixed;
}
.search text{
  position: absolute;
  left:59rpx;
  top:50rpx;
}
.search input{
  width:91%;
  background-color: #fff;
  margin: 37rpx 0;
  padding:0 58rpx;
  border-radius: 30rpx;
  box-shadow: 0px 2px 18px 0px 
    rgba(121, 121, 121, 0.15);
}
.parkPay_main{
  padding-top: 126rpx;
}
.parkPay_main .li{
  background-color: #ffffff;
	box-shadow: 0px 2px 18px 0px rgba(121, 121, 121, 0.15);
  border-radius: 20rpx;
  width:100%;
}
.title{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #333333;
  padding: 20rpx 31rpx;
  border-bottom: 2px solid #efefef;
  margin-bottom: 25rpx;
  line-height: 1;
}
.title_right{
  display: flex;
  align-items: center;
}
.title_right view:nth-child(1){
  font-size: 26rpx;
}
.detail{
  display: flex;
  padding-bottom: 20rpx;
  font-size:26rpx;
  line-height: 1;
}
.detail view:nth-child(1){
  width:21%;
  text-align: right;
  margin-right: 10rpx;
}
.detail view:nth-child(2){
  flex: 1;
}
.tips{
  display: flex;
  justify-content: space-between;
  font-size: 22rpx;
  padding-bottom: 29rpx;
}
.tips view:nth-child(1){
  width:60%;
}
.btn{
  background-color: #ecfcf2;
	border-radius: 41rpx;
  border: solid 1px #009966;
  color: #07ab50;
  padding:13rpx 42rpx;
  line-height: 1;
  font-size: 22rpx;
}
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
 }
  
 .commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  
  color: #333;
  line-height: 1;
 }
 .pay_main{
  padding: 46rpx 31rpx;
  padding-bottom: 0;
 }
 .pay_title{
   font-size: 36rpx;
   text-align: center;
   /* line-height: 1; */
 }
 .integral{
   display: flex;
   justify-content: space-between;
   align-items: center;
   padding-top: 36rpx;
 }
 .integral view:nth-child(1){
   display: flex;
   align-items: center;
 }
 .integral image{
  width: 29rpx;
  height: 35rpx;
  margin-right: 19rpx;
 }
 .select{
  /* width: 35rpx;
	height: 35rpx; */
	background-color: #f5f5f5;
  border: solid 2rpx #d9d9d9;
  border-radius: 50%;
 }
 .integral .integral_num text{
   color:#ff9935;
   font-size: 24rpx
 }
.icon-select{
  color:#07ab50;
  font-size:37rpx;
  line-height: 37rpx;
 }
 .price{
   font-size: 54rpx;
   text-align: center;
   padding-top: 50rpx;
   padding-bottom: 50rpx;
 }
 .pay_way{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:20rpx 25rpx;
  box-shadow: 0px 2px 18px 0px rgba(121, 121, 121, 0.15);
  border-radius: 16rpx;
  margin-bottom: 28rpx;
 }
 .pay_way view:nth-child(1){
  display: flex;
  justify-content: space-between;
  align-items: center;
 }
 .pay_way .image_box{
  margin-right: 30rpx;
}
.image_box image{
  vertical-align: auto;
  width: 52rpx;
  height: 52rpx;
 }
 .pay_money{
   color:#fff;
   background-color: #009933;
   text-align: center;
   padding: 32rpx 0;
   font-size:34rpx;
   line-height:1;
 }
 .text-xl{
   margin-top: 20rpx;
 }