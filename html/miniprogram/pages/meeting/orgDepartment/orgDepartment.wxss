
.orgDepartment{
  margin-top: 29rpx;
}
/* 选择框 */
.orgDepartment .select{
  background-color: #fff;
  padding: 28rpx 30rpx;
  border-bottom: 2px solid #efefef;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
}
.orgDepartment .select text{
  font-size: 35rpx;
  color: #07ab50;
}
.btnBox{
  background-color: #fff;
  position: fixed;
  bottom: 0;
  box-shadow: 0px -1px 25px 0px #efefef;
}
.select_box{
  background-color: #fff;
  margin-bottom:152rpx;
}
.submit{
  background-color: #ecfcf2;
	border-radius: 41px;
  border: solid 1px #009966;
  color: #07ab50;
  margin:33rpx;
  font-size: 30rpx;
  line-height: 1;
  padding:27rpx 312rpx;
}